{
  "version": 2,
  "rewrites": [
    { 
      "source": "/api/productos", 
      "destination": "/api/productos.js" 
    },
    { 
      "source": "/api/productos/:id", 
      "destination": "/api/productos.js?id=:id" 
    },
    { 
      "source": "/(.*)", 
      "destination": "/frontend/$1" 
    }
  ]
}